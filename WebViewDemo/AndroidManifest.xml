<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.webviewdemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Light.NoTitleBar" >
        <activity
            android:name="com.example.webviewdemo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.webviewdemo.WebViewActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.webviewdemo.LocalWebView"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.webviewdemo.XmlActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.webviewdemo.PullXml"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.webviewdemo.SAXXml"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.webviewdemo.DOMXml"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>