<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="textColor" format="color" />
    <attr name="textSize" format="dimension" />
    <attr name="headerBackground" format="reference|color" />

    <declare-styleable name="match">
        <attr name="text" format="string" />
        <attr name="textColor" />
        <attr name="textSize" />
    </declare-styleable>
    <declare-styleable name="SlidingMenu">
        <attr name="mode">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="viewAbove" format="reference" />
        <attr name="viewBehind" format="reference" />
        <attr name="behindOffset" format="dimension" />
        <attr name="behindWidth" format="dimension" />
        <attr name="behindScrollScale" format="float" />
        <attr name="touchModeAbove">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
        </attr>
        <attr name="touchModeBehind">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
        </attr>
        <attr name="shadowDrawable" format="reference" />
        <attr name="shadowWidth" format="dimension" />
        <attr name="fadeEnabled" format="boolean" />
        <attr name="fadeDegree" format="float" />
        <attr name="selectorEnabled" format="boolean" />
        <attr name="selectorDrawable" format="reference" />
    </declare-styleable>

    <attr name="btnColor" format="color" />
    <attr name="mainBackground" format="color" />
    <attr name="mainTextColor" format="reference|color" />
    <attr name="textString" format="string" />

    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textColor" />
        <attr name="textSize" />
        <attr name="max" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
    <declare-styleable name="Indicator">
        <attr name="color" format="color"></attr>
    </declare-styleable>
    <declare-styleable name="myProgress">
        <attr name="myRoundColor" format="color" />
        <attr name="myRoudProgressColor" format="color" />
        <attr name="myRoudWidth" format="dimension" />
        <attr name="myTextColor" format="color" />
        <attr name="myTextSize" format="dimension" />
        <attr name="myShowText" format="boolean" />
        <attr name="myMax" format="integer" />
        <attr name="myStyle">
            <enum name="STROKE" value="0"></enum>
            <!-- enum只能全是大写，否则不能在R.java中生成 -->
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
    <declare-styleable name="MyXlistView">
        <attr name="dividerHeight" format="dimension" />
        <attr name="headerBackground" />
        <attr name="footerBackground" format="color" />
    </declare-styleable>
    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
    </declare-styleable>
    <declare-styleable name="jifen_qiaodao_circle">
        <attr name="radius" format="dimension"></attr>
        <attr name="circle_bg" format="color"></attr>
        <attr name="arc_bg" format="color"></attr>
        <attr name="textcolor" format="color"></attr>
        <attr name="textsize" format="float"></attr>
        <attr name="textmsg" format="string"></attr>
        <attr name="startAngle" format="float"></attr>
        <attr name="sweepAngle" format="float"></attr>
    </declare-styleable>

</resources>